{
  "name": "koa-health-check",
  "version": "0.1.0",
  "description": "Koa Connect middleware for Cloud Health",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm-run-all build:clean build:tsc",
    "build:clean": "rimraf lib",
    "build:tsc": "tsc",
    "test": "npm-run-all test:clean test:build test:mocha",
    "test:clean": "rimraf lib_test",
    "test:build": "tsc -p tsconfig.test.json",
    "test:mocha": "nyc mocha ./lib_test/test/**/*.test.js",
    "postinstall": "npm-run-all build"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.n-fuse.co:10022/restorecommerce/koa-cloud-health.git"
  },
  "keywords": [
    "koa",
    "health",
    "kubernetes"
  ],
  "author": "n-fuse Gmbh",
  "license": "ISC",
  "dependencies": {
    "@cloudnative/health": "^2.0.0",
    "connect": "^3.6.6",
    "koa": "^2.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/koa": "^2.0.48",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.5",
    "@types/sinon": "^7.0.12",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.1"
  }
}
